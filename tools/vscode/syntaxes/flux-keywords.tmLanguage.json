{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Flux Keywords",
  "injectionSelector": "L:source.rust",
  "patterns": [
    {
      "include": "#keywords"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.other.flux",
          "match": "\\bstrg|requires|ensures|refined by|forall|reft\\b"
        }
      ]
    }
  },
  "scopeName": "flux-keywords.injection"
}
