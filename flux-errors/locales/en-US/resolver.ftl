resolver-unsupported-signature =
    unsupported function signature
    .label = {$msg}

resolver-unresolved-path =
    cannot resolve `{$path}`

resolver-mismatched-args =
    mismatched args: rust signature has {$rust_args} args but flux signature has {$flux_args}

resolver-mismatched-type =
    mismatched type: expected `$rust_type` but saw `$flux_type`